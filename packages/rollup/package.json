{
  "name": "@dxyl/rollup",
  "version": "1.0.0",
  "description": "@dxyl/rollup",
  "main": "lib/index.js",
  "module": "esm/index.js",
  "types": "lib/index.d.ts",
  "bin": {
    "dxrollup":"bin/index.js"
  },
  "scripts": {
    "build": "npm run clean && npm run build:esm && npm run build:cjs",
    "build:esm": "node ../../node_modules/typescript/bin/tsc -p tsconfig.json --target ES5 --module ESNext --outDir esm",
    "build:cjs": "node ../../node_modules/typescript/bin/tsc -p tsconfig.json --target ES5 --module CommonJS --outDir lib",
    "clean": "node ../../scripts/del.js del rollup lib esm"
  },
  "files": [
    "lib",
    "src",
    "esm"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fanyonglong/DxBuildProject.git"
  },
  "keywords": [
    "DxBuildProject"
  ],
  "authors": [
    "fanyonglong"
  ],
  "license": "MIT",
  "bugs": "http://github.com/fanyonglong/DxBuildProject/issues",
  "homepage": "https://github.com/fanyonglong/DxBuildProject/tree/master/packages/rollup#readme",
  "devDependencies": {
    "commander": "^5.0.0",
    "resolve-cwd": "^3.0.0",
    "rollup": "^2.7.2",
    "yargs-parser": "^18.1.3",
    "@dxyl/utils":"^1.0.1"
  }
}
